{
  "$schema": "https://ui.shadcn.com/schema/registry",
  "name": "registry-starter-demo",
  "homepage": "https://registry-starter-demo.vercel.app",
  "items": [
    {
      "name": "theme",
      "type": "registry:theme",
      "title": "Dewalt Default Theme",
      "description": "A default Dewalt color theme using Tailwind and shadcn/ui variable names",
      "cssVars": {
        "light": {
          "background": "#FFFFFF",
          "foreground": "#1A1A1A",
          "card": "#FFFFFF",
          "card-foreground": "#1A1A1A",
          "popover": "#FFFFFF",
          "popover-foreground": "#1A1A1A",
          "primary": "#FEBD17",
          "primary-foreground": "#000000",
          "secondary": "#F5F5F5",
          "secondary-foreground": "#353535",
          "muted": "#F5F5F5",
          "muted-foreground": "#6B6B6B",
          "accent": "#275CE2",
          "accent-foreground": "#FFFFFF",
          "destructive": "#CD4040",
          "destructive-foreground": "#FFFFFF",
          "border": "#C4C4C4",
          "input": "#EEEEEE",
          "ring": "#275CE2",
          "chart-1": "#FEBD17",
          "chart-2": "#275CE2",
          "chart-3": "#79AD23",
          "chart-4": "#E68A01",
          "chart-5": "#CD4040",
          "sidebar": "#F5F5F5",
          "sidebar-foreground": "#1A1A1A",
          "sidebar-primary": "#275CE2",
          "sidebar-primary-foreground": "#FFFFFF",
          "sidebar-accent": "#FEBD17",
          "sidebar-accent-foreground": "#000000",
          "sidebar-border": "#C4C4C4",
          "sidebar-ring": "#275CE2",
          "radius": "0.5rem"
        },
        "dark": {
          "background": "#1A1A1A",
          "foreground": "#F5F5F5",
          "card": "#1A1A1A",
          "card-foreground": "#F5F5F5",
          "popover": "#1A1A1A",
          "popover-foreground": "#F5F5F5",
          "primary": "#FEBD17",
          "primary-foreground": "#000000",
          "secondary": "#353535",
          "secondary-foreground": "#F5F5F5",
          "muted": "#353535",
          "muted-foreground": "#A6A6A6",
          "accent": "#275CE2",
          "accent-foreground": "#FFFFFF",
          "destructive": "#CD4040",
          "destructive-foreground": "#F5F5F5",
          "border": "#4D4D4D",
          "input": "#4D4D4D",
          "ring": "#275CE2",
          "chart-1": "#FEBD17",
          "chart-2": "#275CE2",
          "chart-3": "#79AD23",
          "chart-4": "#E68A01",
          "chart-5": "#CD4040",
          "sidebar": "#1A1A1A",
          "sidebar-foreground": "#F5F5F5",
          "sidebar-primary": "#275CE2",
          "sidebar-primary-foreground": "#FFFFFF",
          "sidebar-accent": "#FEBD17",
          "sidebar-accent-foreground": "#000000",
          "sidebar-border": "#4D4D4D",
          "sidebar-ring": "#275CE2",
          "radius": "0.5rem"
        }
      },
      "dependencies": ["radix-ui"],
      "files": [
        {
          "path": "src/app/globals.css",
          "type": "registry:style",
          "target": "app/globals.css"
        },
        {
          "path": "postcss.config.mjs",
          "type": "registry:file",
          "target": "postcss.config.mjs"
        }
      ]
    },
    {
      "name": "dashboard",
      "type": "registry:block",
      "title": "Dashboard",
      "description": "A dashboard application with your brand themed components",
      "registryDependencies": [
        "https://registry-starter-demo.vercel.app/registry/theme", 
        "https://registry-starter-demo.vercel.app/registry/brand-header", 
        "https://registry-starter-demo.vercel.app/registry/brand-footer"],
      "files": [
        {
          "path": "src/layouts/shell-layout.tsx",
          "type": "registry:file",
          "target": "app/layout.tsx"
        },
        {
          "path": "src/app/demo/[name]/blocks/dashboard-page.tsx",
          "type": "registry:page",
          "target": "app/page.tsx"
        }
      ]
    },
    {
      "name": "brand-header",
      "type": "registry:component",
      "title": "Brand Header",
      "description": "A styled, simple, reusable header",
      "registryDependencies": [
        "button",
        "input",
        "avatar",
        "sidebar",
        "https://registry-starter-demo.vercel.app/registry/theme",
        "https://registry-starter-demo.vercel.app/registry/logo"
      ],
      "files": [
        {
          "path": "src/components/brand-header.tsx",
          "type": "registry:component"
        },
        {
          "path": "src/layouts/shell-layout.tsx",
          "type": "registry:file",
          "target": "app/layout.tsx"
        }
      ]
    },
